{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Contoso",
        "websiteUrl": "https://github.com/angrowth/FLW-Teams-AI-Library",
        "privacyUrl": "https://github.com/angrowth/FLW-Teams-AI-Library/blob/main/LICENSE",
        "termsOfUseUrl": "https://github.com/angrowth/FLW-Teams-AI-Library/blob/main/LICENSE"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "My Stocks",
        "full": "My Stocks"
    },
    "description": {
        "short": "Teams AI Chatbot for Stock management.",
        "full": "Teams AI Chatbot for Stock management."
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}